<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="gui.RegionManagerFrameController">
   <children>
      <Label layoutX="75.0" layoutY="60.0" text="Welcom" />
      <Button fx:id="btnUpdateThresholdLevel" layoutX="105.0" layoutY="143.0" mnemonicParsing="false" onAction="#updateThresholdLevel" prefHeight="100.0" prefWidth="260.0" text="Update threshold Level">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnExit" layoutX="43.0" layoutY="553.0" mnemonicParsing="false" onAction="#exit" text="Exit" textFill="RED" />
      <Button fx:id="btnRestockMessage" layoutX="446.0" layoutY="143.0" mnemonicParsing="false" onAction="#RestockMessageToWorker" prefHeight="100.0" prefWidth="260.0" text="Restock message to worker">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnViewReports" layoutX="105.0" layoutY="300.0" mnemonicParsing="false" onAction="#viewReports" prefHeight="100.0" prefWidth="260.0" text="View reports">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
